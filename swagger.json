{
  "swagger": "2.0",
  "info": {
    "version": "1.1",
    "title": "Certificado de Depósito Interbancário Mensal",
    "description": "Taxa de juros entre bancos"
  },
  "host": "victoria-belo.github.io",
  "basePath": "/cdi-app",
  "schemes": ["https"],
  "tags": [
    {
      "name": "CDI"
    }
  ],
  "paths": {
    "/api/v1/cdi": {
      "get": {
        "tags": ["CDI"],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "string",
                    "example": "01/02/2023"
                  },
                  "valor": {
                    "type": "string",
                    "example": "0.92"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  }
}

{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "REST API",
    "description": "Taxa de juros entre bancos"
  },
  "host": "victoria-belo.github.io/cdi-app",
  "basePath": "/",
  "schemes": [   
    "https"
  ],
  "tags": [
    {
      "name": "CDI"
    }
  ],
  "paths": {
    "v1/cdi": {
      "get": {
        "tags": ["CDI"],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "string",
                    "example": "01/02/2023"
                  },
                  "valor": {
                    "type": "string",
                    "example": "0.92"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  }
}
